<h5 >chosen schools graphed against <%= params[:attributename] %> graduation rates</h5></center>
	<div id="graph">	
	</div>

	<%= javascript_include_tag('cs320proj') %>

	
	<div>
	<script type="text/javascript">
		var dataArray=[];
		var namesArray=[];
		<% @data.each do |d| %>

			dataArray.push("<%=d.read_attribute(params[:attributename])%>");
			namesArray.push("<%=d.school_name%>");
			
		<% end %>
		for(var i=0; i<3; i++){
			if(!dataArray[i] && dataArray[i]!=0){
				dataArray[i]=0;
				namesArray[i]='#' + (i+1) + 'entryUnfound';
			}

		}
		
		render_graph(namesArray, dataArray);

	</script>
</div>
